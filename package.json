{
  "name": "n8n-nodes-greenhouse",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Greenhouse ATS providing 16 resources and 100+ operations for candidate management, applications, jobs, offers, interviews, and more. Includes webhook triggers and full GDPR compliance features.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-greenhouse.git"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "greenhouse",
    "ats",
    "applicant-tracking",
    "recruiting",
    "hiring",
    "candidates",
    "hr",
    "human-resources",
    "workflow-automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GreenhouseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Greenhouse/Greenhouse.node.js",
      "dist/nodes/Greenhouse/GreenhouseTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "^1.31.0",
    "n8n-workflow": "^1.31.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
